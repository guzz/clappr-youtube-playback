{
  "name": "clappr-youtube-playback",
  "version": "0.0.1",
  "keywords": [
    "clappr",
    "plugin",
    "youtube"
  ],
  "files": [
    "/dist",
    "/src"
  ],
  "source": "src/clappr-youtube-playback.js",
  "main": "dist/clappr-youtube-playback.js",
  "module": "dist/clappr-youtube-playback.esm.js",
  "scripts": {
    "bundle-check": "ANALYZE_BUNDLE=true rollup --config",
    "build": "rollup --config",
    "release": "MINIMIZE=true rollup --config",
    "prepublishOnly": "npm run release",
    "start": "DEV=true rollup --config --watch",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "test": "jest --coverage --silent",
    "test:coverage": "open coverage/lcov-report/index.html",
    "test:debug": "node --inspect node_modules/.bin/jest --runInBand",
    "test:watch": "jest --watch"
  },
  "devDependencies": {
    "@babel/core": "^7.13.15",
    "@babel/plugin-transform-runtime": "^7.13.15",
    "@babel/preset-env": "^7.13.15",
    "@clappr/core": "^0.4.18",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^18.0.0",
    "@rollup/plugin-node-resolve": "^11.2.1",
    "babel-jest": "^26.6.3",
    "coveralls": "^3.1.0",
    "eslint": "^7.24.0",
    "html-loader": "^2.1.2",
    "jest": "^26.6.3",
    "postcss": "8",
    "rollup": "^2.45.1",
    "rollup-plugin-filesize": "^9.1.1",
    "rollup-plugin-html": "^0.2.1",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-serve": "^1.1.0",
    "rollup-plugin-sizes": "^1.0.4",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-visualizer": "^5.3.1"
  },
  "peerDependencies": {
    "@clappr/core": "^0.4.18"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "not ie <= 11"
  ],
  "dependencies": {
    "youtube-iframe": "^1.0.3"
  }
}
